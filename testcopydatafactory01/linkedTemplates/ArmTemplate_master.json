{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testcopydatafactory01"
		},
		"blobcopytest01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobcopytest01'"
		},
		"ls_desti_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_desti_dl'"
		},
		"ls_sql_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_connection'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=azuresqlservertest001.database.windows.net;Initial Catalog=AzureSQLdatbas01"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"LS_AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azurekeyvaulttest1.vault.azure.net/"
		},
		"LS_HTTP_single_file_breast_cancer_01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_desti_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakergtest01.dfs.core.windows.net/"
		},
		"ls_sqltable_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SqlConnectionAuthentication01"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testcopydatafactory01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blobcopytest01_connectionString": {
						"value": "[parameters('blobcopytest01_connectionString')]"
					},
					"ls_desti_dl_accountKey": {
						"value": "[parameters('ls_desti_dl_accountKey')]"
					},
					"ls_sql_connection_connectionString": {
						"value": "[parameters('ls_sql_connection_connectionString')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"LS_AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_HTTP_single_file_breast_cancer_01_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTP_single_file_breast_cancer_01_properties_typeProperties_url')]"
					},
					"ls_desti_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_desti_dl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "testcopydatafactory01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sqltable_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sqltable_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testcopydatafactory01_ArmTemplate_0')]"
			]
		},
		{
			"name": "testcopydatafactory01_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testcopydatafactory01_ArmTemplate_1')]"
			]
		}
	]
}