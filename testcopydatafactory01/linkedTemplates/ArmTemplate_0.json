		"blobcopytest01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobcopytest01'"
		},
		"ls_desti_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_desti_dl'"
